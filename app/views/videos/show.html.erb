<h2 class="container"><%= @video.title%> - <%= link_to "Edit", edit_video_path(@video) %> - <%= link_to "Delete", video_path(@video), method: :delete %></h2>
<iframe width="560" height="315" src="<%= @video.vid_url %>" frameborder="0" allowfullscreen></iframe>

<div class="comments">
  <h3>Comments</h3>
    <%= link_to "Add new comment", new_video_comment_path(@video) %>
    <% @video.comments.each do |comment| %>
      <div>
        <p>
	        <%= comment.body %>
        </p>
	       <%= link_to "Edit", edit_video_comment_path(@video, comment) %> -
         <%= link_to "Delete", video_comment_path(@video, comment), method: :delete %>
      </div>
    <% end %>
</div>
